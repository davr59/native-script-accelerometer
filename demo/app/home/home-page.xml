<Page class="page" navigatingTo="navigatingTo" xmlns="http://schemas.nativescript.org/tns.xsd">
    <StackLayout>
        <Label text="{{ x, 'X: ' + x }}"/>
        <Label text="{{ y, 'Y: ' + y }}"/>
        <Label text="{{ z, 'Z: ' + z }}"/>
        
        <Button text="start updates" tap="startUpdates" />
        <Button text="stop updates" tap="stopUpdates" />
        
    </StackLayout>
</Page>
